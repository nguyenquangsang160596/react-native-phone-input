{
  "name": "react-native-phone-input-v2",
  "version": "0.2.4",
  "description": "Phone input box for React Native",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "yarn config:dev && expo start",
    "start:web": "yarn config:dev && expo start -w --dev",
    "config:dev": "json -I -f package.json -e 'this.main=\"node_modules/expo/AppEntry.js\"'",
    "config:npm": "json -I -f package.json -e 'this.main=\"lib/index.js\"'",
    "cleaning": "rm -rf node_modules && rm -f yarn.lock && yarn cache clean && yarn install && watchman watch-del-all && rm -fr $TMPDIR/react-*",
    "prepublishOnly": "yarn verify && yarn config:npm",
    "postpublish": "yarn config:dev",
    "deploy": "gh-pages -d web-build",
    "predeploy": "yarn verify && expo build:web && mkdir web-build/countries && cp ./data/countries.json web-build/countries/index.json"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:ravi-poonia/react-native-phone-input.git"
  },
  "keywords": [
    "react-native",
    "phone",
    "telephone",
    "phone input",
    "input",
    "tel",
    "input text",
    "international",
    "react-native-component",
    "ios",
    "android"
  ],
  "dependencies": {
    "google-libphonenumber": "^3.2.15",
    "lodash": "^4.17.20",
    "prop-types": "^15.7.2",
    "react-native-country-picker-modal": "^2.0.0"
  },
  "peerDependencies": {
    "react": "*",
    "react-dom": "*",
    "react-native": "*",
    "react-native-web": "*"
  },
  "author": "Ravi Poonia (https://github.com/ravi-poonia)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ravi-poonia/react-native-phone-input/issues"
  },
  "homepage": "https://github.com/ravi-poonia/react-native-phone-input",
  "devDependencies": {
    "@react-native-community/eslint-config": "^2.0.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.17.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-native": "^3.10.0",
    "expo": "~40.0.0",
    "expo-status-bar": "~1.0.3",
    "json": "^10.0.0",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-native": "https://github.com/expo/react-native/archive/sdk-40.0.1.tar.gz",
    "react-native-web": "~0.13.12"
  }
}
